<%-include('header')-%>

<%-include('navbar')-%>

<form method="post">
    <div class="container">
    <%for(var i=0;i<userdetail.length;i++){%>
        <div class="container my-3">
        <div class="card">
        <div class="card-header text-center">
        <div class="p"><small><i><%=userdetail[i].date%></i></small></div>
        </div>
        <div class="card-body">
        <p class="card-title text-center text-truncate pb-3"><%=userdetail[i].description%></p>
        <a href="/delete/<%=userdetail[i]._id%>" style="font-size: 18px;color:grey"><i class="fa fa-trash" aria-hidden="true"></i></a>
        <a href="/show-diary/<%=userdetail[i]._id %>" class="btn btn-primary float-end"
             style="background-color:rgb(105, 67, 167);border:none">view</a>
        </div>
        </div>
        </div>
        <%}%>
    </div>
</form>




<%-include('footer')-%>